generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

//User with an id as int, & name and email as strings
model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
}

//User with an id as int, & name and email as strings
model Brief {
  id    Int     @id @default(autoincrement())
  title String
  content  String 
}

